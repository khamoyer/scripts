    "version": "2.0.0",
    "tasks": [
        {
            "taskName": "build",
            "isBuildCommand": true,
            "dependsOn": [
                "executable",
                "shared"
            ],
            "problemMatcher": [
                "$gcc"
            ]
        },
        {
            "command": "gcc",
            "isShellCommand": true,
            "isBuildCommand": false,
            "taskName": "executable",
            "args": [
                "synchronousShoping.c",
                "-o",
                "synchronousShoping.exe",
                "-ggdb",
                "-lm"
            ],
            "showOutput": "always"
        }
    ]
}
